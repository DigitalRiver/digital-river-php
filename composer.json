{
    "name": "digitalriver/digital-river-php",
    "version": "1.0.0",
    "description": "Client library for Digital River APIs",
    "keywords": [
        "Digital River",
        "Global Commerce",
        "Client",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://www.digitalriver.com/docs/digital-river-api-reference/",
    "license": "mit",
    "authors": [
        {
            "name": "Digital River",
            "homepage": "https://www.digitalriver.com/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "DigitalRiver\\ApiSdk\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "DigitalRiver\\ApiSdk\\" : "test/" }
    }
}
